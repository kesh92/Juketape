$('#timeline-dashfolio').html('<%= escape_javascript(render partial: "timeline_events/index", locals: {artist: current_user}) %>');
$('.timeline-event-add').on('click', function(){
  $('.timeline-form-panel').slideDown();
  $('.timeline-event-cancel').show();
})

$('.timeline-event-cancel, #timeline-event-cancel').on('click', function(){
  $('.timeline-form-panel').slideUp();
  $('.timeline-event-cancel').hide();
});
$('#timeline-dashfolio .best_in_place').best_in_place();
$('#timeline-dashfolio .datepicker_input').datepicker({
  dateFormat: "dd/mm/yy"
});
