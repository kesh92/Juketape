var data = '<%= escape_javascript(render partial: get_content_partial, locals: { artist: current_user }) %>';
var $updatedHtml = $(data).find('.media-content').html();
$('.' + '<%= params[:container] %>' + ' .media-content').html($updatedHtml);
$('.' + '<%= params[:container] %>').show();
$('.' + '<%= params[:container] %>' + ' #' + '<%= get_tab_content_id %>').fadeIn();
$('#filterContentModal .close').click();
