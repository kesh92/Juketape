<!-- Header section -->
<%= render partial: 'static/header_onboarding' %>


<div class = "search-navigation-bar">	
	<div class = "search-navigation" id= "search-navigation-user">
		<p class = "search-navigation-p"> Search by name <span class = "search-number"> 87</span> </p>

	</div>

	<div class = "search-navigation" id = "search-navigation-tag">
		<p class = "search-navigation-p"> Search by tag  <span class = "search-number"> 65</span></p>
	</div>


</div>


<div class = "container">
	<div class = "row">
